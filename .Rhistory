library(xlsx)
my.wb <- createWorkbook()
my.sheet  <- createSheet(wb=my.wb, sheetName="CellBlock")
my.cb <- CellBlock(sheet=my.sheet, startRow=3, startColumn=2,
noRows = 17, noColumns =  10)
CB.setColData(cellBlock = my.cb, x=1:17, colIndex = 1)
CB.setRowData(cellBlock = my.cb, x=1:10, rowIndex = 1)
# my.fill <- Fill(foregroundColor = "red", backgroundColor="yellow")
# CB.setFill(my.cb, fill=my.fill,rowIndex = seq(2,10,2),
#            colIndex = seq(2,10,2))
# add a matrix, and style it
my.cs <- CellStyle(my.wb) + DataFormat("#,##0.000;[red]-#,##0.000")
x.mat  <- matrix(rnorm(10*6,0,5.3), nrow=10)
my.fill <- Fill(foregroundColor = "blue", backgroundColor="blue")
ind  <- which(x.mat < 0, arr.ind=TRUE)
CB.setFill(my.cb, fill=my.fill, rowIndex = ind[,1]+3,
colIndex=ind[,2]+2)  # note the indices offset
CB.setMatrixData(cellBlock = my.cb, x=x.mat, startRow = 6,
startColumn = 3, cellStyle = my.cs)
saveWorkbook(wb=my.wb, file="./data/test1.xlsx")
